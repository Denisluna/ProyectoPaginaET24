if (typeof err_messages != 'undefined')
  for error in err_messages.err_info
    .msg.err_c
      p #{error}
      .close-msg
        i.fas.fa-times
if (messages.success_msg)
  .msg.scss_c
    p #{messages.success_msg}
    .close-msg
      i.fas.fa-times
if (messages.error)
  .msg.err_c
    p #{messages.error}
    .close-msg
      i.fas.fa-times
